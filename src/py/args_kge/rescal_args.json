{
	"training_data": "data/FB15K/",
  	"output": "../../output/results/",
  	"dataset_division": "",

  	"embedding_module": "TransD",
  	"alignment_module": "",
	  
    "dim": 100,
  	"init": "xavier",
	"ent_l2_norm": true,
	"rel_l2_norm": true,
	"loss_norm": "L2",
	"margin": 1,
	"epsilon": 2,
	"loss": "margin-based",
	"test_tail": false,
	"neg_sampling": "uniform",
	"neg_triple_num": 1,
	  
	"learning_rate": 0.01,
    "optimizer": "Adagrad",
	"max_epoch": 200,
	"batch_size": 5000,

	"batch_threads_num": 2,

	"ordered": true,
	"start_valid": 10,
	"eval_freq": 10,
	"stop_metric": "hits1",
	"top_k": [1, 5, 10, 50],
	"is_save": true,
	"eval_norm": false,
	"is_gpu": true,
	"is_parallel": false,
	"device_number": 1,
	"num_worker": 1
}