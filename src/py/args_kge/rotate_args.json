{
	"training_data": "data/FB15K237/",
  	"output": "../../output/results/",
  	"dataset_division": "",

  	"embedding_module": "RotatE",
  	"alignment_module": "sharing",
	"search_module": "greedy",
	  
    "dim": 125,
  	"init": "normal",
	"ent_l2_norm": true,
	"rel_l2_norm": true,
	"loss_norm": "L2",
	"margin": 6,
	"loss": "logistic_adv",
  	"test_tail": false,
  	"adv":2,
	"neg_sampling": "uniform",
	"neg_triple_num": 25,
	  
	"learning_rate": 0.0005,
    "optimizer": "Adam",
	"max_epoch": 100,
	"batch_size": 1024,

	"batch_threads_num": 2,
	"test_threads_num": 4,

	"ordered": true,
	"start_valid": 110,
	"eval_freq": 10,
	"stop_metric": "hits1",
	"eval_metric": "inner",
	"csls": 10,
	"top_k": [1, 5, 10, 50],
	"is_save": true,
	"eval_norm": false,
	"is_gpu": true,
	"is_parallel": false,
	"device_number": 1,
	"num_worker": 1
}