{
	"training_data": "data/FB15K_type/",
  	"output": "../../output/results/",
  	"dataset_division": "",

  	"embedding_module": "ConnectE",
  	"alignment_module": "mapping",
	"search_module": "greedy",

    "dim": 100,
  	"init": "normal",
	"ent_l2_norm": true,
	"rel_l2_norm": true,
	"loss_norm": "L2",
	"margin": 2,
	"loss": "margin-based",
	"alpha": 5,
	"neg_sampling": "uniform",
	"neg_triple_num": 1,

	"learning_rate": 0.1,
    "optimizer": "Adagrad",
	"max_epoch": 1000,
	"batch_size": 5000,

	"batch_threads_num": 2,
	"test_threads_num": 4,

	"ordered": true,
	"start_valid": 10,
	"eval_freq": 5,
	"stop_metric": "hits1",
	"eval_metric": "inner",
	"csls": 10,
	"top_k": [1, 5, 10, 50],
	"is_save": true,
	"eval_norm": false,
	"is_gpu": true
}