{
	"training_data": "data/OpenEA_dataset_v1.1/EN_DE_15K_V1/",
  	"output": "../../output/results/",
  	"dataset_division": "721_5fold/1/",

  	"embedding_module": "BootEA",
  	"alignment_module": "swapping",
	"search_module": "greedy",
	  
    "dim": 100,
  	"init": "normal",
	"ent_l2_norm": true,
	"rel_l2_norm": true,
	"loss": "limited",
	"loss_norm": "L2",
	  
	"learning_rate": 0.01,
    "optimizer": "Adagrad",
	"max_epoch": 10,
	"batch_size": 5000,
  "margin" : 1,
	"pos_margin": 0.01,
  	"neg_margin": 2.0,
    "neg_margin_balance": 0.2,

  	"neg_sampling": "truncated",
	"neg_triple_num": 10,
	"truncated_epsilon": 0.9,
	"truncated_freq": 10,

	"batch_threads_num": 2,
	"test_threads_num": 4,

	"ordered": true,
	"start_valid": 10,
	"eval_freq": 10,
	"stop_metric": "hits1",
	"eval_metric": "inner",
	"csls": 10,
	"top_k": [1, 5, 10, 50],
	"is_save": true,
	"eval_norm": false,

	"sim_th": 0.7,
	"k": 10,

	"likelihood_slice": 10,
	"sub_epoch": 10,
	"parallel_num": 2,
   "is_gpu":true
}