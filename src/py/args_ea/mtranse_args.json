{
	"training_data": "data/OpenEA_dataset_v1.1/EN_FR_15K_V1/",
  	"output": "../../output/results/",
  	"dataset_division": "721_5fold/1/",

  	"embedding_module": "MTransE",
  	"alignment_module": "mapping",
	"search_module": "greedy",
	  
    "dim": 100,
  	"init": "xavier",
	"ent_l2_norm": true,
	"rel_l2_norm": true,
	"loss_norm": "L2",
	  
	"learning_rate": 0.01,
    "optimizer": "Adam",
	"max_epoch": 1000,
	"batch_size": 5000,

	"alpha": 5,
	"neg_triple_num": 1,
	"batch_threads_num": 2,
	"test_threads_num": 4,

  "margin":1,
	"ordered": true,
	"start_valid": 10,
	"eval_freq": 10,
	"stop_metric": "hits1",
	"eval_metric": "inner",
	"csls": 10,
	"top_k": [1, 5, 10, 50],
	"is_save": true,
	"eval_norm": true,
	"is_parallel": true,
	"parallel_num": 2,
	"is_gpu": false
}